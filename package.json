{
  "name": "attestia",
  "version": "0.1.0",
  "private": true,
  "description": "Financial truth infrastructure for the decentralized world",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:coverage": "pnpm -r run test:coverage",
    "typecheck": "pnpm -r lint",
    "lint": "pnpm -r lint",
    "bench": "pnpm -r run bench",
    "clean": "pnpm -r exec rm -rf dist",
    "demo": "pnpm --filter @attestia/demo run demo"
  },
  "keywords": [
    "attestation",
    "web3",
    "financial-infrastructure",
    "structural-governance",
    "deterministic"
  ],
  "author": "mcp-tool-shop-org",
  "license": "MIT",
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.1"
  }
}
