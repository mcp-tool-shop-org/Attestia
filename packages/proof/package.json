{
  "name": "@attestia/proof",
  "version": "0.2.0",
  "description": "Merkle trees, inclusion proofs, and attestation proof packaging for Attestia",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
