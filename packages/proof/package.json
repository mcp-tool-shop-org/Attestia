  "author": "mcp-tool-shop-org",
  "license": "MIT",
  "dependencies": {
    "json-canonicalize": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}
