{
  "name": "@attestia/sdk",
  "version": "0.2.0",
  "description": "Typed HTTP client SDK for the Attestia API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "attestia",
    "sdk",
    "client",
    "api",
    "verification"
  ],
  "author": "mcp-tool-shop-org",
  "license": "MIT",
  "dependencies": {
    "@attestia/types": "workspace:*"
  },
  "devDependencies": {
    "@attestia/node": "workspace:*",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}
